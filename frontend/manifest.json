{
    "manifest_version": 2,
    "name": "Phishing Detector",
    "version": "1.0",
    "description": "Uma extens√£o para detectar sites de phishing",
    "permissions": [
        "notifications",
        "tabs",
        "http://*/*",
        "https://*/*"
    ],    
    "background": {
        "scripts": ["dist/background.bundle.js"],
        "persistent": true
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["src/content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "src/popup.html",
        "default_icon": {}
    },
    "icons": {}
}
